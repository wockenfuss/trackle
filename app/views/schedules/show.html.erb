<div id="city">
	<%= render 'sidebar' %>
</div>

<div id="usersList">
	<% if @users.any? %>
	<div id="usersContainer">
		<% @users.each do |user| %>
			<%= render :partial => 'users/box', :locals => { :user => user } %>
		<% end %>		
	</div>
	<% end %>
</div>
<div id="user-modal" class="modal"></div>
<div id="task-modal" class="modal">
	<%= render 'assignments/form' %>
</div>